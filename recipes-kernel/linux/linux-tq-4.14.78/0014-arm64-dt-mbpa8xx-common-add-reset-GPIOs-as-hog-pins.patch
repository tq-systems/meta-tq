From 71748cf551646b211268d68750de5785bb7fda08 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Mon, 13 May 2019 12:40:33 +0200
Subject: [PATCH] arm64: dt: mbpa8xx-common: add reset GPIOs as hog pins

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi | 11 ++++++++++-
 1 file changed, 10 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi b/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi
index b702c8f0ca73..c05c6599b596 100644
--- a/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi
+++ b/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi
@@ -71,7 +71,7 @@
 
 &iomuxc {
 	pinctrl-names = "default";
-	pinctrl-0 = <&pinctrl_hog>;
+	pinctrl-0 = <&pinctrl_hog>, <&pinctrl_rstgpio>;
 
 	tqma8xx-mbpa8xx {
 		pinctrl_fec1: fec1grp {
@@ -111,6 +111,15 @@
 			>;
 		};
 
+		pinctrl_rstgpio: rstgpiogrp {
+			fsl,pins = <
+				/* GNSS_RST */
+				SC_P_SPI0_CS1_LSIO_GPIO1_IO07			0x06000041
+				/* WWAN_RST */
+				SC_P_ADC_IN5_LSIO_GPIO1_IO13			0x06000041
+			>;
+		};
+
 		pinctrl_led: ledggrp {
 			fsl,pins = <
 				SC_P_CSI_RESET_LSIO_GPIO3_IO03			0x06000048
