From 21958d39f5b7bb8f67c60e487287ac32d145fe25 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Wed, 6 Feb 2019 15:56:41 +0100
Subject: [PATCH] arm64: dt: fsl-imx8qxp-tqma8qx-mba8qx: fix RGMII skew

fixes jira TQMA8XX-43

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 .../boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi    | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi
index 136e262e60fc..2bdd1236914e 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi
@@ -289,8 +289,8 @@
 		ethphy0: ethernet-phy@0 {
 			compatible = "ethernet-phy-ieee802.3-c22";
 			reg = <0>;
-			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_50_NS>;
-			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_50_NS>;
+			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
+			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,dp83867-rxctrl-strap-quirk;
 			ti,led-function = <0xc100>; /* LED1: Link, LED2: activity */
 			ti,led-ctrl = <0x1001>;     /* active low, LED1/2 driven by phy */
@@ -299,8 +299,8 @@
 		ethphy3: ethernet-phy@3 {
 			compatible = "ethernet-phy-ieee802.3-c22";
 			reg = <3>;
-			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_50_NS>;
-			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_50_NS>;
+			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
+			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,dp83867-rxctrl-strap-quirk;
 			ti,led-function = <0xc100>; /* LED1: Link, LED2: activity */
 			ti,led-ctrl = <0x1001>;     /* active low, LED1/2 driven by phy */
