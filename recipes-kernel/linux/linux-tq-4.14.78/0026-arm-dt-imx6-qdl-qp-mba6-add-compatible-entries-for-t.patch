From d2d73993fa1bc516818e686feac8a10c40acc0f7 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Mon, 10 Sep 2018 11:12:17 +0200
Subject: [PATCH 026/188] arm: dt: imx6<qdl,qp>-mba6: add compatible entries
 for tqma6

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/boot/dts/imx6q-mba6a.dts  | 2 +-
 arch/arm/boot/dts/imx6q-mba6b.dts  | 2 +-
 arch/arm/boot/dts/imx6qp-mba6b.dts | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/boot/dts/imx6q-mba6a.dts b/arch/arm/boot/dts/imx6q-mba6a.dts
index b22999149d56..2cf9d93c2e23 100644
--- a/arch/arm/boot/dts/imx6q-mba6a.dts
+++ b/arch/arm/boot/dts/imx6q-mba6a.dts
@@ -20,5 +20,5 @@
 
 / {
 	model = "TQ TQMa6Q on MBa6x";
-	compatible = "tq,mba6a", "fsl,imx6q";
+	compatible = "tq,mba6a", "tq,tqma6q", "fsl,imx6q";
 };
diff --git a/arch/arm/boot/dts/imx6q-mba6b.dts b/arch/arm/boot/dts/imx6q-mba6b.dts
index 725e745a102e..8a0f74864e8d 100644
--- a/arch/arm/boot/dts/imx6q-mba6b.dts
+++ b/arch/arm/boot/dts/imx6q-mba6b.dts
@@ -20,5 +20,5 @@
 
 / {
 	model = "TQ TQMa6Q on MBa6x";
-	compatible = "tq,mba6b", "fsl,imx6q";
+	compatible = "tq,mba6b", "tq,tqma6q", "fsl,imx6q";
 };
diff --git a/arch/arm/boot/dts/imx6qp-mba6b.dts b/arch/arm/boot/dts/imx6qp-mba6b.dts
index 5288332f0cc8..b58506e9d706 100644
--- a/arch/arm/boot/dts/imx6qp-mba6b.dts
+++ b/arch/arm/boot/dts/imx6qp-mba6b.dts
@@ -18,5 +18,5 @@
 
 / {
 	model = "TQ TQMa6QP on MBa6x";
-	compatible = "tq,mba6b", "fsl,imx6q";
+	compatible = "tq,mba6b", "tq,tqma6qp", "fsl,imx6qp";
 };
