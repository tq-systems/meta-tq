From 039185030c161c39c8fe3aa50b6d63d6f17d74ae Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Wed, 30 Jan 2019 16:52:31 +0100
Subject: [PATCH] arm64: dt: fsl-imx8mq-tqma8mq: enable mu but leave rpmsg
 disabled

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi | 11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi
index 3d25c3c802b9..6621af7f44bf 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi
@@ -55,6 +55,9 @@
 			alloc-ranges = <0 0x40000000 0 0x1C000000>;
 			linux,cma-default;
 		};
+
+		/* disable until we fix it */
+		/delete-node/ rpmsg@0x78000000;
 	};
 };
 
@@ -254,6 +257,10 @@
 	};
 };
 
+&mu {
+	status = "okay";
+};
+
 /* TODO: not working */
 &qspi {
 	pinctrl-names = "default";
@@ -270,6 +277,10 @@
 	};
 };
 
+&rpmsg{
+	status = "disabled";
+};
+
 &usdhc1 {
 	pinctrl-names = "default", "state_100mhz", "state_200mhz";
 	pinctrl-0 = <&pinctrl_usdhc1>;
