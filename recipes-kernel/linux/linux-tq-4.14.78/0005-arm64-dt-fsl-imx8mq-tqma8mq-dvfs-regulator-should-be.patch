From 4214a73916e16dbee49f905f56351f3519f6397d Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Wed, 14 Aug 2019 14:40:53 +0200
Subject: [PATCH] arm64: dt: fsl-imx8mq-tqma8mq: dvfs regulator should be
 active low

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi
index def1738cefff..1f7be0a5fe0f 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8mq-tqma8mq.dtsi
@@ -69,7 +69,7 @@
 		regulator-name = "TQMa8Mx_DVFS";
 		regulator-type = "voltage";
 		regulator-settling-time-us = <150000>;
-		gpios = <&gpio1 6 GPIO_ACTIVE_HIGH>;
+		gpios = <&gpio1 6 GPIO_ACTIVE_LOW>;
 		states = <900000 0x1 1000000 0x0>;
 		status = "okay";
 	};
