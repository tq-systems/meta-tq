From 4da74af586209711d84fb64381947f3d63a54f57 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Tue, 7 May 2019 12:56:13 +0200
Subject: [PATCH] arm64: dt: tqma8xxs [cosmetic]: use tqma8xxs in iomux groups

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm64/boot/dts/freescale/mb-smarc-2-common.dtsi | 2 +-
 arch/arm64/boot/dts/freescale/tqma8xxs-common.dtsi   | 5 ++++-
 2 files changed, 5 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/mb-smarc-2-common.dtsi b/arch/arm64/boot/dts/freescale/mb-smarc-2-common.dtsi
index 0954a030b319..e107ce0f096c 100644
--- a/arch/arm64/boot/dts/freescale/mb-smarc-2-common.dtsi
+++ b/arch/arm64/boot/dts/freescale/mb-smarc-2-common.dtsi
@@ -50,7 +50,7 @@
 };
 
 &iomuxc {
-	tqma8qxs-mb-smarc-2 {
+	tqma8xxs-mb-smarc-2 {
 		pinctrl_lpuart0: lpuart0grp {
 			fsl,pins = <
 				SC_P_UART0_RX_ADMA_UART0_RX	0x06000020
diff --git a/arch/arm64/boot/dts/freescale/tqma8xxs-common.dtsi b/arch/arm64/boot/dts/freescale/tqma8xxs-common.dtsi
index 06c8ea114def..4b2919edea4c 100644
--- a/arch/arm64/boot/dts/freescale/tqma8xxs-common.dtsi
+++ b/arch/arm64/boot/dts/freescale/tqma8xxs-common.dtsi
@@ -197,7 +197,10 @@
 };
 
 &iomuxc {
-	imx8qxp-tqma8qxs {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_hog>;
+
+	tqma8xxs {
 		pinctrl_fec1: fec1grp {
 			fsl,pins = <
 				SC_P_ENET0_MDC_CONN_ENET0_MDC			0x06000048
