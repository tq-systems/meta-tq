+TQ System device tree bindings
+----------------------------------------
+
+------------
+TQMa6 module
+------------
+
+TQMa6-MBa6dl variant a
+	compatible = "tq,mba6a", "tq,tqma6dl", "fsl,imx6dl";
+
+TQMa6-MBa6dl variant b
+	compatible = "tq,mba6a", "tq,tqma6dl", "fsl,imx6dl";
+
+TQMa6-MBa6q variant a
+	compatible = "tq,mba6a", "tq,tqma6q", "fsl,imx6q";
+
+TQMa6-MBa6q variant b
+	compatible = "tq,mba6b", "tq,tqma6q", "fsl,imx6q";
+
+TQMa6-MBa6qp
+	compatible = "tq,mba6b", "tq,tqma6qp", "fsl,imx6qp";
