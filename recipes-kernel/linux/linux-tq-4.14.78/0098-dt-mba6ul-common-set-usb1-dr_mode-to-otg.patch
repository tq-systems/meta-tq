From: Michael Krummsdorf <michael.krummsdorf@tq-group.com>
Date: Wed, 6 Feb 2019 13:49:34 +0100
Subject: [PATCH 098/188] dt: mba6ul-common: set usb1 dr_mode to otg

Signed-off-by: Michael Krummsdorf <michael.krummsdorf@tq-group.com>
---
 arch/arm/boot/dts/mba6ul-common.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/mba6ul-common.dtsi b/arch/arm/boot/dts/mba6ul-common.dtsi
index 0c21ad0e381a..360afe9ca658 100644
--- a/arch/arm/boot/dts/mba6ul-common.dtsi
+++ b/arch/arm/boot/dts/mba6ul-common.dtsi
@@ -635,7 +635,7 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_usb_otg1>;
 	vbus-supply = <&reg_otg1vbus_5v0>;
-	dr_mode = "host";
+	dr_mode = "otg";
 	status = "okay";
 };
 
