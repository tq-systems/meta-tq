From ff418a46540fed9fd7a504860bec730f0c91fad7 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Tue, 14 May 2019 16:21:48 +0200
Subject: [PATCH] arm64: dt: mbpa8xx-common: fix ethernet PHY leds

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi | 6 ++++--
 1 file changed, 4 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi b/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi
index d62189599300..e284320ab3fd 100644
--- a/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi
+++ b/arch/arm64/boot/dts/freescale/mbpa8xx-common.dtsi
@@ -225,8 +225,10 @@
 			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,dp83867-rxctrl-strap-quirk;
-			ti,led-cfg1 = <0xc100>; /* LED1: Link, LED2: activity */
-			ti,led-cfg2 = <0x1001>; /* active low, LED1/2 driven by phy */
+			/* LED1: Link / activity, LED2: GiGe */
+			ti,led-cfg1 = <0x05b0>;
+			/* 0/3: low, LED1/2 inverted, driven by phy */
+			ti,led-cfg2 = <0x1001>;
 		};
 	};
 };
