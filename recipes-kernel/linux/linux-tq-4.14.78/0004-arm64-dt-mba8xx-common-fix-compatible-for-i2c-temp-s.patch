From dfc34143392673e96bc5d2b690896283c0686f23 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Wed, 8 May 2019 08:55:00 +0200
Subject: [PATCH 4/7] arm64: dt: mba8xx-common: fix compatible for i2c temp
 sensor

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm64/boot/dts/freescale/mba8xx-common.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/freescale/mba8xx-common.dtsi b/arch/arm64/boot/dts/freescale/mba8xx-common.dtsi
index c84f7826c652..a6d72e1a97dc 100644
--- a/arch/arm64/boot/dts/freescale/mba8xx-common.dtsi
+++ b/arch/arm64/boot/dts/freescale/mba8xx-common.dtsi
@@ -167,7 +167,7 @@
 	 * with hwmon sysfs ABI and break device registration
 	 */
 	sensor1: jc42@1c {
-		compatible = "jc42";
+		compatible = "nxp,se97", "jedec,jc-42.4-temp";
 		reg = <0x1c>;
 		status = "okay";
 	};
