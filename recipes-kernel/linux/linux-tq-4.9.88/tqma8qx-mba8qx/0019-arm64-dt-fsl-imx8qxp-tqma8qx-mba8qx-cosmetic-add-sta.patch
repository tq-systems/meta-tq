From e04d66191e48212c68ff57b730772e1bc49eb246 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Tue, 30 Oct 2018 16:42:33 +0100
Subject: [PATCH] arm64: dt: fsl-imx8qxp-tqma8qx-mba8qx: [cosmetic] add status
 to all regulator nodes

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 .../boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi     | 7 +++++--
 1 file changed, 5 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi
index 2698b62f6ee5..859f5b676c71 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8qxp-tqma8qx-mba8qx.dtsi
@@ -60,14 +60,16 @@
 			enable-active-high;
 			startup-delay-us = <100>;
 			off-on-delay-us = <12000>;
+			status = "okay";
 		};
 
-		reg_mba8qx_v5v: mba8qx_v5v {
+		reg_mba8qx_5v: mba8qx_5v {
 			compatible = "regulator-fixed";
 			regulator-name = "MBA8QX_V5V";
 			regulator-min-microvolt = <5000000>;
 			regulator-max-microvolt = <5000000>;
 			regulator-always-on;
+			status = "okay";
 		};
 
 		reg_mba8qx_12v: mba8qx_12v {
@@ -77,6 +79,7 @@
 			regulator-max-microvolt = <12000000>;
 			gpio = <&expander 6 GPIO_ACTIVE_HIGH>;
 			enable-active-high;
+			status = "okay";
 		};
 	};
 };
@@ -296,7 +299,7 @@
 &usbotg1 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_usbotg1>;
-	vbus-supply = <&reg_mba8qx_v5v>;
+	vbus-supply = <&reg_mba8qx_5v>;
 	srp-disable;
 	hnp-disable;
 	adp-disable;
