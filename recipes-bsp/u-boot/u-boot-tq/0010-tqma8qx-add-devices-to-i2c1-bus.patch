From d61559c113c422411d8ecd4489616bd29c8e8be7 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Thu, 25 Oct 2018 12:15:43 +0200
Subject: [PATCH] tqma8qx: add devices to i2c1 bus

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi | 14 ++++++++++++++
 1 file changed, 14 insertions(+)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi b/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
index 6c8b6b93ce..8994f08599 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
@@ -159,6 +159,20 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_lpi2c1>;
 	status = "okay";
+
+	eeprom1: 24c02@51 {
+		compatible = "atmel,24c02";
+		reg = <0x51>;
+		pagesize = <16>;
+		status = "okay";
+	};
+
+	eeprom0: m24c64@57 {
+		compatible = "st,24c64", "atmel,24c64", "at24";
+		reg = <0x57>;
+		pagesize = <32>;
+		status = "okay";
+	};
 };
 
 &usdhc1 {
