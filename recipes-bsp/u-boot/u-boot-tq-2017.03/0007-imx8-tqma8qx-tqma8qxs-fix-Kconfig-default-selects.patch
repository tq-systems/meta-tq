From 29351eca8445762c7706e03353f6ec9f017add62 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Thu, 17 Jan 2019 15:31:38 +0100
Subject: [PATCH] imx8: tqma8qx / tqma8qxs: fix Kconfig default selects

remove CONFIG_ prefix added by accident

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/cpu/armv8/imx8/Kconfig | 20 ++++++++++----------
 board/tqc/tqma8qx/Kconfig       |  1 -
 2 files changed, 10 insertions(+), 11 deletions(-)

diff --git a/arch/arm/cpu/armv8/imx8/Kconfig b/arch/arm/cpu/armv8/imx8/Kconfig
index 0a1d182c0e..23cec0cea1 100644
--- a/arch/arm/cpu/armv8/imx8/Kconfig
+++ b/arch/arm/cpu/armv8/imx8/Kconfig
@@ -68,21 +68,21 @@ config TARGET_TQMA8QX
 	bool "Support TQMa8xX board"
 	select BOARD_LATE_INIT
 	select IMX8QXP
-	select CONFIG_DM_GPIO
-	select CONFIG_DM_ETH
-	select CONFIG_DM_SERIAL
-	select CONFIG_FSL_LPUART
-	select CONFIG_DM_MMC
+	select DM_GPIO
+	select DM_ETH
+	select DM_SERIAL
+	select FSL_LPUART
+	select DM_MMC
 
 config TARGET_TQMA8QXS
 	bool "Support TQMa8xXS board"
 	select BOARD_LATE_INIT
 	select IMX8QXP
-	select CONFIG_DM_GPIO
-	select CONFIG_DM_ETH
-	select CONFIG_DM_SERIAL
-	select CONFIG_FSL_LPUART
-	select CONFIG_DM_MMC
+	select DM_GPIO
+	select DM_ETH
+	select DM_SERIAL
+	select FSL_LPUART
+	select DM_MMC
 
 endchoice
 
diff --git a/board/tqc/tqma8qx/Kconfig b/board/tqc/tqma8qx/Kconfig
index 029a25ee6a..9699a6d76e 100644
--- a/board/tqc/tqma8qx/Kconfig
+++ b/board/tqc/tqma8qx/Kconfig
@@ -22,7 +22,6 @@ config TQMA8QX_MX8QXP
 
 endchoice
 
-
 choice
 	prompt "TQMa8QX base board variant"
 	default MBA8QX
