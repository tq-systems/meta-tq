+	reg32_write(0x3c04c288,0x5);
+	reg32_write(0x3c04c28c,0x7);
+	reg32_write(0x3c04c290,0x4);
+	reg32_write(0x3c04c294,0x6);
+	reg32_write(0x3c04c298,0x3);
+	reg32_write(0x3c04c29c,0x0);
