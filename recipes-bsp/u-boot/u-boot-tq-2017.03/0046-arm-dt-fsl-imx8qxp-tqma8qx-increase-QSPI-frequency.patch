From f5e08f9ac83a25a4b4b4cb1f75c69ce09c407296 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Thu, 7 Feb 2019 16:02:48 +0100
Subject: [PATCH] arm: dt: fsl-imx8qxp-tqma8qx: increase QSPI frequency

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi b/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
index e8789d4c2f..601332c139 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
@@ -125,12 +125,12 @@
 	pinctrl-0 = <&pinctrl_flexspi0>;
 	status = "okay";
 
-	flash0: mt35xu512aba@0 {
+	flash0: flash@0 {
 		reg = <0>;
 		#address-cells = <1>;
 		#size-cells = <1>;
 		compatible = "spi-flash";
-		spi-max-frequency = <29000000>;
+		spi-max-frequency = <66000000>;
 		spi-nor,ddr-quad-read-dummy = <8>;
 	};
 };
