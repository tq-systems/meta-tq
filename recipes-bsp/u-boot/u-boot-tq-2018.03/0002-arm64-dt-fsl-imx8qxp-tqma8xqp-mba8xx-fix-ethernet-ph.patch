From db5188614468d2f7e521a79e3dabb6e4ec58094c Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Wed, 15 May 2019 17:57:50 +0200
Subject: [PATCH] arm64: dt: fsl-imx8qxp-tqma8xqp-mba8xx: fix ethernet phy led

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8xqp-mba8xx.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mba8xx.dts b/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mba8xx.dts
index 6eb23c75ff..acd67628d9 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mba8xx.dts
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mba8xx.dts
@@ -178,8 +178,8 @@
 			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,dp83867-rxctrl-strap-quirk;
-			ti,led-function = <0xc100>; /* LED1: Link, LED2: activity */
-			ti,led-ctrl = <0x1001>;     /* active low, LED1/2 driven by phy */
+			ti,led-cfg1 = <0x0100>; /* LED1: Link, LED2: activity */
+			ti,led-cfg2 = <0x1001>; /* active low, LED1/2 driven by phy */
 		};
 
 		ethphy3: ethernet-phy@3 {
@@ -188,8 +188,8 @@
 			ti,rx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,tx-internal-delay = <DP83867_RGMIIDCTL_2_25_NS>;
 			ti,dp83867-rxctrl-strap-quirk;
-			ti,led-function = <0xc100>; /* LED1: Link, LED2: activity */
-			ti,led-ctrl = <0x1001>;     /* active low, LED1/2 driven by phy */
+			ti,led-cfg1 = <0x0100>; /* LED1: Link, LED2: activity */
+			ti,led-cfg2 = <0x1001>; /* active low, LED1/2 driven by phy */
 		};
 	};
 };
