From 3b2f0734ccfa1f92ef8bd4c294a4d1a7d231e243 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Thu, 14 Feb 2019 06:43:43 +0100
Subject: [PATCH] arm: dt: fsl-imx8qxp-tqma8qxs: [cosmetic] sync usdhc with
 kernel

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8qxs-mb-smarc-2.dts | 4 +++-
 arch/arm/dts/fsl-imx8qxp-tqma8qxs.dtsi           | 4 ++--
 2 files changed, 5 insertions(+), 3 deletions(-)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qxs-mb-smarc-2.dts b/arch/arm/dts/fsl-imx8qxp-tqma8qxs-mb-smarc-2.dts
index 4f48d2dc4f..b7d3efd985 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qxs-mb-smarc-2.dts
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qxs-mb-smarc-2.dts
@@ -14,8 +14,8 @@
 
 /dts-v1/;
 
-#include "fsl-imx8qxp-tqma8qxs.dtsi"
 #include <dt-bindings/net/ti-dp83867.h>
+#include "fsl-imx8qxp-tqma8qxs.dtsi"
 
 / {
 	model = "TQ Systems i.MX8QXP TQMa8xXS on MB-SMARC-2";
@@ -39,6 +39,8 @@
 			gpio = <&gpio4 19 GPIO_ACTIVE_LOW>;
 			startup-delay-us = <100>;
 			off-on-delay-us = <12000>;
+			regulator-always-on;
+			regulator-boot-on;
 		};
 
 		reg_usdhc2_vqmmc: usdhc2_vqmmc {
diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qxs.dtsi b/arch/arm/dts/fsl-imx8qxp-tqma8qxs.dtsi
index 7a19909c0c..4ed19023b2 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qxs.dtsi
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qxs.dtsi
@@ -190,9 +190,9 @@
 	pinctrl-0 = <&pinctrl_usdhc1>;
 	pinctrl-1 = <&pinctrl_usdhc1_100mhz>;
 	pinctrl-2 = <&pinctrl_usdhc1_200mhz>;
-	bus-width = <8>;
-	non-removable;
 	vmmc-supply = <&reg_usdhc1_vmmc>;
 	vqmmc-supply = <&reg_usdhc1_vqmmc>;
+	bus-width = <8>;
+	non-removable;
 	status = "okay";
 };
