From 8f59091fa540f98137c1e03df6c069f2712242ea Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Mon, 19 Nov 2018 12:53:31 +0100
Subject: [PATCH] arm: dt: fsl-imx8qxp-tqma8qx / fsl-imx8qxp-tqma8qx-mba8qx:
 fix board compatible

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts | 2 +-
 arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi       | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts b/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts
index d635b0a33f..16228159e9 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts
@@ -19,7 +19,7 @@
 
 / {
 	model = "TQ Systems i.MX8QXP TQMa8xX on MBa8xX";
-	compatible = "tqc,tqma8qx-mba8qx", "tqc,tqma8qx", "fsl,imx8qxp";
+	compatible = "tq,tqma8qx-mba8qx", "tq,tqma8qx", "fsl,imx8qxp";
 
 	chosen {
 		bootargs = "console=ttyLP1,115200 earlycon=lpuart32,0x5a070000,115200";
diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi b/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
index 34246dd08a..dca3fcb9ff 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qx.dtsi
@@ -20,7 +20,7 @@
 
 / {
 	model = "TQ Systems i.MX8QXP TQMa8xX";
-	compatible = "fsl,tqma8qx", "fsl,imx8qxp";
+	compatible = "tq,tqma8qx", "fsl,imx8qxp";
 
 	regulators {
 		compatible = "simple-bus";
