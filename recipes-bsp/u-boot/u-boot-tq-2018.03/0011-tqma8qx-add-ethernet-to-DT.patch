From f1875363d6a50e8ebfd87d8b668c2ccb55944f14 Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Thu, 4 Oct 2018 12:21:36 +0200
Subject: [PATCH] tqma8qx: add ethernet to DT

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts | 14 ++++++--------
 1 file changed, 6 insertions(+), 8 deletions(-)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts b/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts
index 55699a3126..4cc443c9f5 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8qx-mba8qx.dts
@@ -160,9 +160,8 @@
 	pinctrl-0 = <&pinctrl_fec1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy0>;
-	fsl,ar8031-phy-fixup;
-	fsl,magic-packet;
-	status = "disabled";
+/*	fsl,magic-packet; */
+	status = "okay";
 
 	mdio {
 		#address-cells = <1>;
@@ -173,9 +172,9 @@
 			reg = <0>;
 		};
 
-		ethphy1: ethernet-phy@1 {
+		ethphy3: ethernet-phy@3 {
 			compatible = "ethernet-phy-ieee802.3-c22";
-			reg = <1>;
+			reg = <3>;
 		};
 	};
 };
@@ -184,9 +183,8 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_fec2>;
 	phy-mode = "rgmii";
-	phy-handle = <&ethphy1>;
-	fsl,ar8031-phy-fixup;
-	fsl,magic-packet;
+	phy-handle = <&ethphy3>;
+/*	fsl,magic-packet; */
 	status = "okay";
 };
 
