 board/tqc/tqma8xxs/tqma8xxs-mb-smarc-2.c | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/board/tqc/tqma8xxs/tqma8xxs-mb-smarc-2.c b/board/tqc/tqma8xxs/tqma8xxs-mb-smarc-2.c
index f4dd3156bb..079eb121e8 100644
