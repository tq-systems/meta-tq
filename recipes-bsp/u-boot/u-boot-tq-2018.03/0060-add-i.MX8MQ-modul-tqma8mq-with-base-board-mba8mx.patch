diff --git a/board/tqc/tqma8mx/Kconfig b/board/tqc/tqma8mx/Kconfig
new file mode 100644
index 0000000000..8348b011ee
--- /dev/null
+++ b/board/tqc/tqma8mx/Kconfig
@@ -0,0 +1,56 @@
+if TARGET_TQMA8MX
+
+config SYS_BOARD
+	default "tqma8mx"
+
+config SYS_VENDOR
+	default "tqc"
+
+config SYS_CONFIG_NAME
+	default "tqma8mx"
+
+choice
+	prompt "TQMa8MX CPU variant"
