From ab7e7784a326f55a2d560ee2b51955be462c9b4d Mon Sep 17 00:00:00 2001
From: Markus Niebel <Markus.Niebel@tq-group.com>
Date: Mon, 13 May 2019 13:21:25 +0200
Subject: [PATCH] arm: dt: add reset GPIO as hog pins

Signed-off-by: Markus Niebel <Markus.Niebel@tq-group.com>
---
 arch/arm/dts/fsl-imx8qxp-tqma8xqp-mbpa8xx.dts | 11 ++++++++++-
 1 file changed, 10 insertions(+), 1 deletion(-)

diff --git a/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mbpa8xx.dts b/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mbpa8xx.dts
index 75300ef685..177ec8915a 100644
--- a/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mbpa8xx.dts
+++ b/arch/arm/dts/fsl-imx8qxp-tqma8xqp-mbpa8xx.dts
@@ -69,7 +69,7 @@
 
 &iomuxc {
 	pinctrl-names = "default";
-	pinctrl-0 = <&pinctrl_hog>;
+	pinctrl-0 = <&pinctrl_hog>, <&pinctrl_rstgpio>;
 
 	tqma8xx-mbpa8xx {
 		pinctrl_lpuart1: lpuart1grp {
@@ -118,6 +118,15 @@
 			>;
 		};
 
+		pinctrl_rstgpio: rstgpiogrp {
+			fsl,pins = <
+				/* GNSS_RST */
+				SC_P_SPI0_CS1_LSIO_GPIO1_IO07			0x06000041
+				/* WWAN_RST */
+				SC_P_ADC_IN5_LSIO_GPIO1_IO13			0x06000041
+			>;
+		};
+
 		pinctrl_usbotg1: usbotg1grp {
 			fsl,pins = <
 				SC_P_USB_SS3_TC0_CONN_USB_OTG1_PWR	0x06000020
